              <Label fx:id="label" alignment="CENTER" minHeight="16" minWidth="69" prefHeight="98.0" prefWidth="320.0" text="Physics Mini Games" textAlignment="CENTER">
               <font>
                  <Font name="Cambria Math" size="22.0" />
               </font>
            </Label>
              <Button fx:id="game1" alignment="CENTER" onAction="#startMiniGame1" prefHeight="25.0" prefWidth="154.0" text="Play &quot;Charge Cannon&quot; " textAlignment="CENTER" />
            <Button fx:id="game2" mnemonicParsing="false" onAction="#startMiniGame2" prefHeight="25.0" prefWidth="156.0" text="Play Mini Game 2" />
            <Button fx:id="game3" mnemonicParsing="false" onAction="#startMiniGame3" prefHeight="25.0" prefWidth="157.0" text="Play Mini Game 3" />
