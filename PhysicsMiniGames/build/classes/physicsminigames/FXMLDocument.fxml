<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="physicsminigames.FXMLDocumentController">
    <children>
      <VBox fx:id="box" alignment="TOP_CENTER" layoutX="98.0" layoutY="3.0" prefHeight="493.0" prefWidth="315.0" spacing="50.0">
