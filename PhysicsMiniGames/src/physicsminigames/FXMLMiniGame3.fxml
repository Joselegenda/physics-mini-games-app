<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="pane" onMouseClicked="#dropObject" onMouseMoved="#MouseMoved" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="physicsminigames.FXMLMiniGame3Controller">
   <children>
      <Label fx:id="levelLabel" layoutX="611.0" layoutY="510.0" prefHeight="17.0" prefWidth="109.0" text="Level: 1" textFill="WHITE" />
      <Label fx:id="lensLabel" layoutX="144.0" layoutY="80.0" text="Lens" textFill="WHITE" />
      <Label fx:id="mirrorLabel" layoutX="65.0" layoutY="80.0" text="Mirror" textFill="WHITE" />
      <Label fx:id="diamondLabel" layoutX="435.0" layoutY="80.0" text="Diamond" textFill="WHITE" />
      <Label fx:id="amberLabel" layoutX="511.0" layoutY="80.0" text="Amber" textFill="WHITE" />
      <Label fx:id="statusLabel" layoutX="181.0" layoutY="555.0" prefHeight="17.0" prefWidth="385.0" text="Pick an object and place it on the screen" textFill="#f8f8f8" />
      <Button fx:id="mirrorBtn" layoutX="57.0" layoutY="32.0" mnemonicParsing="false" onAction="#handleMirror" prefHeight="41.0" prefWidth="49.0" />
      <Button fx:id="lensBtn" layoutX="131.0" layoutY="32.0" mnemonicParsing="false" onAction="#handleLens" prefHeight="41.0" prefWidth="49.0" />
      <Button fx:id="diamondBtn" layoutX="435.0" layoutY="31.0" mnemonicParsing="false" onAction="#handleDiamond" prefHeight="41.0" prefWidth="49.0" />
      <Button fx:id="amberBtn" layoutX="504.0" layoutY="31.0" mnemonicParsing="false" onAction="#handleAmber" prefHeight="41.0" prefWidth="49.0" />
      <Button id="startBtn" layoutX="36.0" layoutY="551.0" mnemonicParsing="false" onAction="#handleStartBtn" text="Start" />
      <Button id="resetBtn" layoutX="94.0" layoutY="551.0" mnemonicParsing="false" onAction="#handleResetBtn" text="Reset" />
      <Button id="nextLevelBtn" fx:id="nextLevelBtn" layoutX="611.0" layoutY="539.0" mnemonicParsing="false" onAction="#handleNextLevelBtn" text="Next Level" />
      <Button id="quitBtn" fx:id="quitBtn" layoutX="653.0" layoutY="26.0" mnemonicParsing="false" onAction="#handleQuitBtn" text="Quit" />
      <Slider fx:id="chosenFocalLength" layoutX="233.0" layoutY="30.0" min="20.0" prefHeight="17.0" prefWidth="140.0" value="40.0" />
      <Button fx:id="helpBtn" layoutX="590.0" layoutY="26.0" mnemonicParsing="false" text="Help" />
      <Label fx:id="focalLength" alignment="CENTER" layoutX="233.0" layoutY="53.0" prefHeight="17.0" prefWidth="140.0" text="Focal Length: 40" textFill="WHITE" />
      <Button fx:id="restartGameBtn" layoutX="611.0" layoutY="569.0" mnemonicParsing="false" onAction="#handleRestartGame" text="Restart Game" />
   </children>
</AnchorPane>
